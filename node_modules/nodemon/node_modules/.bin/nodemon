#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/bin/node_modules:/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/node_modules:/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/nodemon@3.1.0/node_modules:/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/bin/node_modules:/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/node_modules:/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/nodemon@3.1.0/node_modules:/mnt/c/Users/romec/OneDrive - UNIVERSIDAD DE LAS FUERZAS ARMADAS ESPE/U/Tesis/lab_login/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
